{
  "_version": "1.65.0",
  "sap.app": {
    "id": "smartdesk",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.5",
      "toolsId": "ec9f2211-67e8-42f6-a8e6-51341bece944"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/smart-desk/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.136.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "smartdesk.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "smartdesk.view",
        "async": true,
        "viewPath": "smartdesk.view"
      },
      "routes": [
        {
          "name": "RouteHomePage",
          "pattern": ":?query:",
          "target": [
            "TargetHomePage"
          ]
        },
        {
          "name": "TimeTracking",
          "pattern": "TimeTracking/{Id}",
          "target": [
            "TargetTimeTrackingView"
          ]
        },
        {
          "name": "Workflow",
          "pattern": "Workflow/{Id}",
          "target": [
            "TargetWorkflowView"
          ]
        },
        {
          "name": "PayStatement",
          "pattern": "PayStatement/{Id}",
          "target": [
            "TargetPayStatementView"
          ]
        },
        {
          "name": "ViewProfile",
          "pattern": "ViewProfile/{Id}",
          "target": [
            "TargetViewProfileView"
          ]
        },
        {
          "name": "ViewLearning",
          "pattern": "ViewLearning/{Id}",
          "target": [
            "TargetViewLearningView"
          ]
        },
        {
          "name": "ViewOrg",
          "pattern": "ViewOrg/{Id}",
          "target": [
            "TargetViewOrgView"
          ]
        },
        {
          "name": "ViewLeaves",
          "pattern": "ViewLeaves/{Id}",
          "target": [
            "TargetViewLeaves"
          ]
        },
        {
          "name": "ViewTileReport",
          "pattern": "ViewTileReport/{Id}",
          "target": [
            "TargetViewTileReports"
          ]
        }
      ],
      "targets": {
        "TargetHomePage": {
          "id": "HomePage",
          "name": "HomePage"
        },
        "TargetTimeTrackingView": {
          "viewType": "XML",
          "transition": "slide",
          "id": "TimeTracking",
          "name": "TimeTracking"
        },
        "TargetWorkflowView": {
          "viewType": "XML",
          "transition": "slide",
          "id": "Workflow",
          "name": "Workflow"
        },
        "TargetPayStatementView": {
          "viewType": "XML",
          "transition": "slide",
          "id": "PayStatement",
          "name": "PayStatement"
        },
        "TargetViewProfileView": {
          "viewType": "XML",
          "transition": "slide",
          "id": "ViewProfile",
          "name": "ViewProfile"
        },
        "TargetViewLearningView": {
          "viewType": "XML",
          "transition": "slide",
          "id": "ViewLearning",
          "name": "ViewLearning"
        },
        "TargetViewOrgView": {
          "viewType": "XML",
          "transition": "slide",
          "id": "ViewOrg",
          "name": "ViewOrg"
        },
        "TargetViewLeaves": {
          "viewType": "XML",
          "transition": "slide",
          "id": "ViewLeaves",
          "name": "ViewLeaves"
        },
        "TargetViewTileReports": {
          "viewType": "XML",
          "transition": "slide",
          "id": "ViewTileReport",
          "name": "ViewTileReport"
        }
      }
    },
    "rootView": {
      "viewName": "smartdesk.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}